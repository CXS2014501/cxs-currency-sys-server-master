<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cxs.mapper.FilePathMapper">

    <resultMap id="BaseResultMap" type="com.cxs.model.FilePath">
            <id property="fileId" column="file_id" jdbcType="VARCHAR"/>
            <result property="fileOrignName" column="file_orign_name" jdbcType="VARCHAR"/>
            <result property="fileRealPath" column="file_real_path" jdbcType="VARCHAR"/>
            <result property="fileAccessUrl" column="file_access_url" jdbcType="VARCHAR"/>
            <result property="fileSize" column="file_size" jdbcType="BIGINT"/>
            <result property="createDate" column="create_date" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        file_id,file_orign_name,file_real_path,file_size,
        file_access_url,create_date
    </sql>
</mapper>
